<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:subViewModels="clr-namespace:ApplicationLayer.ViewModels.SubViewModels;assembly=ApplicationLayer.ViewModels"
                    xmlns:subViews="clr-namespace:ApplicationLayer.Views.SubViews;assembly=ApplicationLayer.Views"
                    xmlns:optionViewModels="clr-namespace:ApplicationLayer.ViewModels.DialogViewModels.OptionViewModels;assembly=ApplicationLayer.ViewModels"
                    xmlns:optionViews="clr-namespace:ApplicationLayer.Views.DialogViews.OptionViews;assembly=ApplicationLayer.Views"
                    xmlns:local="clr-namespace:WpfApp.Resources.DataTemplates"
                    xmlns:localViewModels="clr-namespace:WpfApp.ViewModels.WindowViewModels"
                    xmlns:localViews="clr-namespace:WpfApp.Views">

    <DataTemplate DataType="{x:Type subViewModels:ProjectSelectionViewModel}">
        <subViews:ProjectSelectionView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type optionViewModels:FontsAndColorsViewModel}">
        <optionViews:FontsAndColorsView/>
    </DataTemplate>

</ResourceDictionary>